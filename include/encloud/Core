namespace libencloud {

class Config;

class Core : public QObject
{
    Q_OBJECT

public:
    Core ();
    ~Core ();

    int start ();
    int stop ();

private:

    int initConfig();

    Config *_config; 
};

}  // namespace libencloud

#endif  // _LIBENCLOUD_CORE_H_
